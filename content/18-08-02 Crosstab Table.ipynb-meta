    "# Import modules\n",
    "import pandas as pd\n",
    "\n",
    "# Create a dataframe\n",
    "df = pd.DataFrame(data=[['Red','Red','Red','Blue','Green'],\n",
    "                        ['Honda','Acura','Honda','Nissan','Nissan']])\n",
    "df = df.T\n",
    "df.columns = ['Color','Model']\n",
    "\n",
    "# View the datafram\n",
    "df"
